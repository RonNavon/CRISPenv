{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "Environment": {
            "type": "string"
        },
        "Environment2": {
            "type": "string"
        },
        "Vnet": {
            "type": "string"
        },
        "SUB-VDS": {
            "type": "string"
        },
        "SUB-AKS": {
            "type": "string"
        },
        "SubscriptionID": {
            "type": "string"
        },
        "mongodb": {
            "type": "string"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            /**CrispMongoDBEndpoint*/
            "type": "Microsoft.Network/privateEndpoints",
            "apiVersion": "2020-11-01",
            "name": "[concat('CrispMongoDBEndpoint', parameters('Environment'))]",
            "location": "[resourceGroup().location]",
            "properties": {
                "privateLinkServiceConnections": [
                    {
                        "name": "[concat('CrispMongoDBEndpoint', parameters('Environment'))]",
                        "properties": {
                            "privateLinkServiceId": "[concat('/subscriptions/', parameters('SubscriptionID'),'/resourceGroups/DATA', parameters('Environment'),'/providers/Microsoft.DocumentDb/databaseAccounts/', parameters('mongodb'), parameters('Environment'))]",
                            "groupIds": [
                                "MongoDB"
                            ],
                            "privateLinkServiceConnectionState": {
                                "status": "Approved",
                                "actionsRequired": "None"
                            }
                        }
                    }
                ],
                "manualPrivateLinkServiceConnections": [],
                "subnet": {
                    "id": "[concat('/subscriptions/', parameters('SubscriptionID'),'/resourceGroups/Network', parameters('Environment'),'/providers/Microsoft.Network/virtualNetworks/', parameters('Vnet'), parameters('Environment') ,'/subnets/', parameters('SUB-AKS'), parameters('Environment'))]"
                },
                "customDnsConfigs": []
            }
        },
        {
            "type": "Microsoft.Network/privateEndpoints/privateDnsZoneGroups",
            "apiVersion": "2020-11-01",
            "name": "[concat('CrispMongoDBEndpoint', parameters('Environment'), '/default')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/privateEndpoints', concat('CrispMongoDBEndpoint', parameters('Environment')))]"
            ],
            "properties": {
                "privateDnsZoneConfigs": [
                    {
                        "name": "privatelink-mongo-cosmos-azure-com",
                        "properties": {
                            "privateDnsZoneId": "[concat('/subscriptions/', parameters('SubscriptionID'), '/resourceGroups/Network', parameters('Environment'),'/providers/Microsoft.Network/privateDnsZones/privatelink.mongo.cosmos.azure.com')]"
                        }
                    }
                ]
            }
        }
        /**CrispRedisEndpoint*/
        /**crispvdsstoragecrt*/

    ],
    "outputs": {}
}