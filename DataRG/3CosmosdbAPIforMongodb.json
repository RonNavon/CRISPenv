            "name": "[concat(parameters('mongodb'), parameters('Environment'))]",
            "type": "Microsoft.DocumentDB/databaseAccounts",
            "apiVersion": "2021-05-15",
            "location": "[resourceGroup().location]",
            "tags": {
                "defaultExperience": "Azure Cosmos DB for MongoDB API",
                "hidden-cosmos-mmspecial": ""
            },
            "kind": "MongoDB",
            "identity": {
                "type": "None"
            },
            "properties": {
                "publicNetworkAccess": "Enabled",
                "enableAutomaticFailover": false,
                "enableMultipleWriteLocations": false,
                "isVirtualNetworkFilterEnabled": true,
                "virtualNetworkRules": [
                    {
                        "id": "[concat('/subscriptions/', parameters('SubscriptionID'), '/resourceGroups/Network', parameters('Environment'),'/providers/Microsoft.Network/virtualNetworks/', parameters('Vnet'), parameters('Environment'),'/subnets/', parameters('SUB-AKS'), parameters('Environment'))]",
