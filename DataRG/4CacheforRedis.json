{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
          "Environment": {
            "type": "string"
        },
        "Environment2": {
            "type": "string"
        },
        "Vnet": {
            "type": "string"
        },
        "SUB-VDS": {
            "type": "string"
        },
        "SUB-AKS": {
            "type": "string"
        },
        "SubscriptionID": {
            "type": "string"
        },
        "mongodb": {
            "type": "string"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
         {
            "type": "Microsoft.Cache/Redis",
            "apiVersion": "2021-06-01",
            "name": "[concat('CrispRedis', parameters('Environment'))]",
            "location": "West Europe",
            "properties": {
                "redisVersion": "6.0.14",
                "sku": {
                    "name": "Standard",
                    "family": "C",
                    "capacity": 3
                },
                "enableNonSslPort": false,
                "publicNetworkAccess": "Disabled",
                "redisConfiguration": {
                    "maxmemory-reserved": "642",
                    "maxfragmentationmemory-reserved": "642",
                    "maxmemory-delta": "642"
                }
            }
        },
        {
            "type": "Microsoft.Cache/Redis/privateEndpointConnections",
            "apiVersion": "2021-06-01",
            "name": "[concat('CrispRedis', parameters('Environment'), '/CrispRedisEndpoint', parameters('Environment'),'.d2d2bccb-be6d-420f-9560-6d76ce368436')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cache/Redis', concat('CrispRedis', parameters('Environment')))]"
            ],
            "properties": {
                "privateEndpoint": {},
                "privateLinkServiceConnectionState": {
                    "status": "Approved",
                    "description": "Auto-Approved",
                    "actionsRequired": "None"
                }
            }
        }
    ],
    "outputs": {}
}