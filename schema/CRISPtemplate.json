{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "EnvironmentName": {
      "type": "string",
      "metadata": {
        "description": "acronyms of the new environment"
      }
    }
  },
  "functions": [],
  "variables": {
    "VnetName": "CrispVnet"
  },
